<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lorcan.spinnerwheel.demo">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">

    <activity android:name="com.lorcan.spinnerwheel.demo.WheelDemo">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity
        android:name="com.lorcan.spinnerwheel.demo.DateTimePickerActivity"
        android:label="Date Time Picker">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="com.lorcan.spinnerwheel.WHEEL_SAMPLE"/>
      </intent-filter>
    </activity>
    <activity
        android:name="com.lorcan.spinnerwheel.demo.TimePickerActivity"
        android:label="Time picker">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="com.lorcan.spinnerwheel.WHEEL_SAMPLE"/>
      </intent-filter>
    </activity>
    <activity
        android:name="com.lorcan.spinnerwheel.demo.TimePickerCustomViewsActivity"
        android:label="Time picker with custom views">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="com.lorcan.spinnerwheel.WHEEL_SAMPLE"/>
      </intent-filter>
    </activity>
    <activity
        android:name="com.lorcan.spinnerwheel.demo.ProgrammaticSwitchingActivity"
        android:label="Programmatic switching (shuffle)">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="com.lorcan.spinnerwheel.WHEEL_SAMPLE"/>
      </intent-filter>
    </activity>
    <activity
        android:name="com.lorcan.spinnerwheel.demo.CitiesActivity"
        android:label="Repopulating data">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="com.lorcan.spinnerwheel.WHEEL_SAMPLE"/>
      </intent-filter>
    </activity>
    <activity
        android:name="com.lorcan.spinnerwheel.demo.ColorPickerActivity"
        android:label="Color picker with custom views">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="com.lorcan.spinnerwheel.WHEEL_SAMPLE"/>
      </intent-filter>
    </activity>

  </application>


</manifest>